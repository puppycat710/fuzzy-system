</script>
<script>
  function closeWindow() {
    window.opener = null;
    window.open('', '_self');
    window.close();
    window.history.back();
  }
  document.onkeydown = function (e) {
    if (event.keyCode == 123) {
      closeWindow()
      return false;
    }
    if (e.ctrlKey && e.shiftKey && e.keyCode == 'I'.charCodeAt(0)) {
      closeWindow()
      return false;
    }
    if (e.ctrlKey && e.shiftKey && e.keyCode == 'J'.charCodeAt(0)) {
      closeWindow()
