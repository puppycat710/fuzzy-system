<!DOCTYPE html>
<html amp lang="pt-br">
  <head>
    {% include "seo.njk" %}
    {% include "seoSerie.njk" %}
    <!-- AMP -->
    <script async src="https://cdn.ampproject.org/v0.js"></script>
    <script async custom-element="amp-iframe" src="https://cdn.ampproject.org/v0/amp-iframe-0.1.js"></script>
    <script async custom-element="amp-mega-menu" src="https://cdn.ampproject.org/v0/amp-mega-menu-0.1.js"></script>
    <script async custom-element="amp-bind" src="https://cdn.ampproject.org/v0/amp-bind-0.1.js"></script>
    <script src="https://cdn.tailwindcss.com"></script>
    {% include "styles.njk" %}
    <style amp-boilerplate>
      body {
        -webkit-animation: -amp-start 8s steps(1, end) 0s 1 normal both;
        -moz-animation: -amp-start 8s steps(1, end) 0s 1 normal both;
        -ms-animation: -amp-start 8s steps(1, end) 0s 1 normal both;
        animation: -amp-start 8s steps(1, end) 0s 1 normal both;
      }
      @-webkit-keyframes -amp-start {
        from {
          visibility: hidden;
        }
        to {
          visibility: visible;
        }
      }
      @keyframes -amp-start {
        from {
          visibility: hidden;
        }
        to {
          visibility: visible;
        }
      }
    </style>
    <noscript>
      <style amp-boilerplate>
        body {
          -webkit-animation: none;
          -moz-animation: none;
          -ms-animation: none;
          animation: none;
        }
      </style>
    </noscript>
    {% include "adsense.njk" %}
    {# {% include "devTool.njk" %} #}
  </head>
  <body>
    {% include "headerMenu.njk" %}
    {{ content | safe }}
    {% include "footer.njk" %}
    <!-- Carregue o JS ap처s o HTML -->
    <script>
      document.getElementById('play-button').addEventListener('click', () => { // Clique no elemento com id="video-frame"
        const videoFrame = document.getElementById('video-frame');
        if (videoFrame) {
          videoFrame.click();
          console.log('Clique no video-frame realizado.');
        } else {
          console.error('Elemento com id "video-frame" n찾o encontrado.');
        }
        // Clique no elemento com data-fp="r28bnykxp2"
        const dataFpElement = document.querySelector('[data-fp="r28bnykxp2"]');
        if (dataFpElement) {
          dataFpElement.click();
          console.log('Clique no elemento com data-fp="r28bnykxp2" realizado.');
        } else {
          console.error('Elemento com data-fp="r28bnykxp2" n찾o encontrado.');
        }
        // Clique na div recap-icon
        const recapIcon = document.getElementById('recap-icon');
        if (recapIcon) {
          recapIcon.click();
          console.log('Clique na div recap-icon realizado.');
        } else {
          console.error('Elemento com id "recap-icon" n찾o encontrado.');
        }
      });
    </script>
    <script defer src="/js/monetag-vignatte.js" defer></script>
    <script defer src="/js/render-banner.js" defer></script>
    <script defer src="/js/play-btn.js" defer></script>
    <script defer src="/js/disqus.js" defer></script>
    <script defer src="/js/search.js" defer></script>
    <script defer src="/js/amp.js" defer></script>
  </body>
</html>